{
  "$schema": "https://biomejs.dev/schemas/2.3.10/schema.json",
  "extends": ["./base.json"],
  "linter": {
    "rules": {
      "suspicious": {
        "noExplicitAny": "off",
        "noConfusingVoidType": "off"
      },
      "complexity": {
        "noUselessFragments": "off"
      },
      "style": {
        "noNonNullAssertion": "off"
      }
    }
  }
}
